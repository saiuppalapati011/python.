#Write a class which takes a number n and generates divisibles of 11.

class divisibles_of_11:
  def __init__(self,n):
    self.n=n
  def divisibles_11(self):
    list1=[]
    for i in range(1,self.n+1):
      if i%11==0:
        list1.append(i)
    print(list1)

n=int(input("Enter a number:"))
d=divisibles_of_11(n)
d.divisibles_11()
