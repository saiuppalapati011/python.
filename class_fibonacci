
#Write a program using a class to generate fibonacci series.
class Fibonacci:
  def __init__(self,num):
    self.num=num
  def fibonacci(self):
    num=int(input("Enter a Number:\n"))
    sum=0
    a=0
    b=1
    print("Fibonacci Series:")
    print(a)
    for i in range(1,num+1):
      print(b)
      sum=a+b
      a=b
      b=sum

F2=Fibonacci(num)
F2.fibonacci()
