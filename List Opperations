print(names[-4])
names[2]="tia"
print(names[2])
print(names[0:3])
names.append("ajay")
print(names)
names.insert(0,"rama")
print(names)
names.remove('nadeem')
print(names)
print("rama" in names)
print(len(names))
names.clear()
print(names)
