a = [1,2,3,'ram ji','shyam ji', 2.3]
type(a)
print(a)
print(a[0])
print(a[-1])
print(a[-0])
print(a[1:3])
print(a[0:6])
print(a[2:])
print(a[0:6:2])
print(a[:-1])
print(a[-1:])
names = ["sajid","nadeem","touseef","sahid","ram ji"]
